{
  "installCommand": "pnpm install",
  "buildCommand": "tsc -p backend/tsconfig.json && pnpm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/$1"
    }
  ]
}